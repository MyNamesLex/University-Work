--skeleton code--

function _init()
	modegame = {update=updategame,draw=drawgame}
	modegameover = {update=updategameover, draw=drawgameover,}
	initgame()
end

function _update60()
	updategame()
end

function _draw()
	cls()
	map(0,0,0,0,16,16)
	mode.draw()
end
