--initgame, updategame,drawgame--	
function initgame()
sfx(2)
	mode = modegame
	survivor = {x=100,y=100,cooldown=0}
	bats = {}
	enemies = {}
	bomb = {}
	end

function updategame()
	updatesurvivor()
	updatebat()
	updateenemies()
	updatebomb()
	--collision--
	enemycollision()
	survivorcollision()
end

function drawgame()
	drawsurvivor()
	drawbat()
	drawenemies()
	drawbomb()
end
