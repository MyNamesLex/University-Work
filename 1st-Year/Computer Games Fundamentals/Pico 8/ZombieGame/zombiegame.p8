--survivor functions--
function fire(x, y) 
		if survivor.cooldown > 0 then 
			return 
		end 
		add(bats, {x=x, y=y}) 
		survivor.cooldown=40
		sfx(0)
		end

function updatesurvivor()
if btn(0) and survivor.x > 0 then
 survivor.x -= 1
 spr (38,survivor.x,survivor.y,2,2)
 end

if btn(1) and survivor.x < 112 then
survivor.x += 1
end

if btn(2) and survivor.y > 0 then
survivor.y -= 1
end

if btn(3) and survivor.y < 112 then
survivor.y += 1
end

survivor.cooldown -= 1
if btn(4) then
fire(survivor.x+8, survivor.y+6)
end
end

function drawsurvivor()
spr (32,survivor.x,survivor.y,2,2)

if btn(1) then
spr (36,survivor.x,survivor.y,2,2)
end

if btn(2) then 
spr (34,survivor.x,survivor.y,2,2)
end

if btn(3) then 
spr (32,survivor.x,survivor.y,2,2) 
end

if btn(0) then 
spr (38,survivor.x,survivor.y,2,2) 
end

end


