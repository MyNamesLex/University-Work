function updateenemies()
 spawnenemiestop()
 spawnenemiesleft()
 spawnenemiesright()
 for e in all(enemies) do
 e.x += e.xspeed
 e.y += e.yspeed
 -- remove off screen enemies
 if e.y > 128 then
 del(enemies, e)
 end
 end
end

function drawenemies()
	for enemy in all (enemies) do
		spr (enemy.sprite,enemy.x,enemy.y,enemy.ss,enemy.ss)
	end
end
